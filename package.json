{
  "ava": {
    "babel": {
      "presets": []
    },
    "concurrency": 5,
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "private": true,
  "main": "./dist/src/index.js",
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:*",
    "build:ts": "tsc --outDir dist",
    "build:assets": "cpx \"./{scripts,src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "clean": "rimraf dist",
    "online-service": "node ./dist/src/bin/online-service",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "devDependencies": {
    "@types/azure": "^0.9.19",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.106",
    "@types/multiparty": "0.0.31",
    "@types/node": "^9.6.0",
    "@types/passport": "^0.4.3",
    "ava": "^0.25.0",
    "cpx": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-typescript": "^0.12.0",
    "eventemitter2": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "typescript-eslint-parser": "^14.0.0"
  },
  "dependencies": {
    "@sonarwhal/configuration-web-recommended": "^5.0.0",
    "@sonarwhal/configuration-progressive-web-apps": "^1.0.0",
    "@sonarwhal/rule-image-optimization-cloudinary": "^3.0.1",
    "@sonarwhal/rule-optimize-image": "^1.0.1",
    "applicationinsights": "^1.0.2",
    "azure-sb": "^0.10.6",
    "azure-storage": "^2.8.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "globby": "^8.0.1",
    "handlebars": "^4.0.11",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "moment": "^2.22.0",
    "mongodb-lock": "^0.4.0",
    "mongoose": "^5.0.10",
    "multiparty": "^4.1.3",
    "node-schedule": "^1.3.0",
    "ntp-time-sync": "^0.1.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "request": "^2.85.0",
    "sonarwhal": "^1.5.0",
    "strip-bom": "^3.0.0",
    "strip-json-comments": "^2.0.1",
    "tar": "^4.4.1",
    "tri": "^1.0.2",
    "uuid": "^3.2.1"
  },
  "nyc": {
    "branches": 83,
    "cache": true,
    "check-coverage": true,
    "functions": 80,
    "include": [
      "dist/src/**/*.js"
    ],
    "instrument": true,
    "lines": 85,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "statements": 85
  },
  "version": "0.16.0"
}
