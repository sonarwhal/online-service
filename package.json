{
  "ava": {
    "babel": {
      "presets": []
    },
    "concurrency": 5,
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "private": true,
  "main": "./dist/src/index.js",
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:*",
    "build:ts": "tsc --outDir dist",
    "build:assets": "cpx \"./{scripts,src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "clean": "rimraf dist",
    "online-service": "node ./dist/src/bin/online-service",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "devDependencies": {
    "@types/azure": "^0.9.19",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.88",
    "@types/multiparty": "0.0.31",
    "@types/node": "^9.3.0",
    "@types/passport": "^0.4.1",
    "ava": "^0.25.0",
    "cpx": "^1.5.0",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-markdown": "^1.0.0-beta.7",
    "eslint-plugin-typescript": "^0.8.1",
    "eventemitter2": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "^4.0.0",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^12.0.0"
  },
  "dependencies": {
    "applicationinsights": "^1.0.1",
    "azure-sb": "^0.10.4",
    "azure-storage": "^2.7.0",
    "body-parser": "^1.17.2",
    "connect-mongo": "^2.0.0",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "globby": "^7.1.1",
    "handlebars": "^4.0.10",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "moment": "^2.19.4",
    "mongodb-lock": "^0.4.0",
    "mongoose": "^4.13.9",
    "multiparty": "^4.1.3",
    "node-schedule": "^1.3.0",
    "ntp-time-sync": "^0.1.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "proxyquire": "^1.8.0",
    "request": "^2.83.0",
    "sinon": "^4.1.3",
    "sonarwhal": "^0.24.0",
    "strip-bom": "^3.0.0",
    "strip-json-comments": "^2.0.1",
    "tar": "^4.3.0",
    "tri": "^1.0.2",
    "uuid": "^3.1.0"
  },
  "nyc": {
    "branches": 83,
    "cache": true,
    "check-coverage": true,
    "functions": 80,
    "include": [
      "dist/src/**/*.js"
    ],
    "instrument": true,
    "lines": 85,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "statements": 85
  },
  "version": "0.16.0"
}
