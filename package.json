{
  "ava": {
    "babel": {
      "presets": []
    },
    "concurrency": 5,
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "private": true,
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "npm run clean && npm-run-all build:*",
    "build:ts": "tsc --outDir dist",
    "build:assets": "cpx \"./{scripts,src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "clean": "rimraf dist",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.72",
    "@types/mongoose": "^4.7.19",
    "@types/multiparty": "0.0.31",
    "@types/node": "^8.0.19",
    "ava": "^0.21.0",
    "cpx": "^1.5.0",
    "eslint": "^4.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-typescript": "^0.8.0",
    "eventemitter2": "^4.1.2",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^8.0.0"
  },
  "dependencies": {
    "@sonarwhal/sonar": "^0.11.0",
    "azure-sb": "^0.10.4",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb-lock": "^0.4.0",
    "mongoose": "^4.11.6",
    "multiparty": "^4.1.3",
    "proxyquire": "^1.8.0",
    "sinon": "^4.0.0",
    "strip-bom": "^3.0.0",
    "strip-json-comments": "^2.0.1",
    "tri": "^1.0.2",
    "uuid": "^3.1.0"
  },
  "nyc": {
    "branches": 84,
    "cache": true,
    "check-coverage": true,
    "functions": 80,
    "include": [
      "dist/src/**/*.js"
    ],
    "instrument": true,
    "lines": 85,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "statements": 85
  },
  "version": "0.0.1"
}
