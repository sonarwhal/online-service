{
  "ava": {
    "babel": {
      "presets": []
    },
    "concurrency": 5,
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "@hint/configuration-progressive-web-apps": "^1.1.1",
    "@hint/configuration-web-recommended": "^2.0.0",
    "@hint/hint-image-optimization-cloudinary": "^1.0.3",
    "@hint/hint-minified-js": "^1.0.2",
    "@hint/hint-no-broken-links": "^1.0.7",
    "@hint/hint-no-p3p": "^1.0.4",
    "@hint/hint-performance-budget": "^1.0.3",
    "applicationinsights": "^1.0.6",
    "azure-sb": "^0.10.6",
    "azure-storage": "^2.10.2",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "globby": "^8.0.1",
    "handlebars": "^4.0.12",
    "hint": "^3.4.10",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mongodb-lock": "^0.4.0",
    "mongoose": "^5.3.4",
    "multiparty": "^4.2.1",
    "node-schedule": "^1.3.0",
    "ntp-time-sync": "^0.1.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "request": "^2.88.0",
    "strip-bom": "^3.0.0",
    "strip-json-comments": "^2.0.1",
    "tar": "^4.4.6",
    "tri": "^1.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/azure": "^0.9.20",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.117",
    "@types/multiparty": "0.0.31",
    "@types/node": "10.9.1",
    "@types/passport": "^0.4.6",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^5.0.5",
    "ava": "^0.25.0",
    "cpx": "^1.5.0",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-typescript": "^0.12.0",
    "eventemitter2": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "proxyquire": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Chore: Update `${dependency}` to `v${version}`",
      "devDependencyUpdate": "Chore: Update `${dependency}` to `v${version}`",
      "lockfileUpdate": "Chore: Update lockfile ${lockfilePath}"
    },
    "ignore": [
      "@types/node"
    ]
  },
  "main": "./dist/src/index.js",
  "nyc": {
    "branches": 83,
    "cache": true,
    "check-coverage": true,
    "functions": 80,
    "include": [
      "dist/src/**/*.js"
    ],
    "instrument": true,
    "lines": 85,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "statements": 85
  },
  "private": true,
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:*",
    "build:assets": "cpx \"./{scripts,src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc --outDir dist",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "online-service": "node ./dist/src/bin/online-service",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "0.16.0"
}
